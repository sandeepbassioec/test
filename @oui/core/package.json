{
	"name": "@oui/core",
	"version": "0.9.1",
	"description": "Oui Component library",
	"main": "dist/index.cjs.js",
	"module": "dist/index.js",
	"es2015": "dist/esm/index.mjs",
	"es2017": "dist/esm/index.mjs",
	"jsnext:main": "dist/esm/index.js",
	"types": "dist/types/interfaces.d.ts",
	"collection": "dist/collection/collection-manifest.json",
	"collection:main": "dist/collection/index.js",
	"unpkg": "dist/core/core.esm.js",
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/ionic-team/stencil-component-starter.git"
	},
	"files": [
		"LICENSE",
		"README.md",
		"components/",
		"dist/",
		"loader/",
		"hydrate/",
		"!__mocks__/",
		"!stories/"
	],
	"scripts": {
		"build": "stencil build --prod && pnpm run post-build && npx tsx scripts/fix-react-events.ts",
		"post-build": "run-p post-build:*",
		"post-build:styles": "pnpm run build:public-styles && pnpm run copy:public-styles",
		"post-build:themes": "node scripts/copy-themes.js",
		"post-build:mocks": "pnpm run copy:jest-mocks",
		"dev": "pnpm dev-stencil && pnpm dev-storybook",
		"dev-stencil": "pnpm start",
		"lint": "eslint .",
		"start": "pnpm run build:dev-public-styles && stencil build --dev --watch --serve --config stencil.dev.config.ts",
		"test": "pnpm jest",
		"test.coverage": "pnpm jest --coverage",
		"test.watch": "pnpm jest --watchAll",
		"watch:tailwind-dev": "pnpm tailwindcss -i ./src/styles/tailwind.css -o ./www/build/core.css --watch",
		"generate": "stencil generate",
		"dev-storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"release": "pnpm build && pnpm publish",
		"pack": "pnpm pack",
		"build:public-styles": "npx tailwindcss -i ./src/global/oui.css -o ./dist/core/oui.css --minify",
		"build:dev-public-styles": "npx tailwindcss -i ./src/global/oui.css -o ./public-styles/oui.css --minify",
		"copy:public-styles": "cpx ./dist/core/oui.css ../react/src/global/",
		"copy:jest-mocks": "cpx ../jest-mocks/oui-mocks.js ./dist/ && cpx ../jest-mocks/jest-setup.js ./dist/ && cpx ../jest-mocks/jest.config.js ./dist/"
	},
	"dependencies": {
		"@awesome.me/kit-a1ac577d64": "^1.0.63",
		"@floating-ui/dom": "^1.6.13",
		"@fortawesome/fontawesome-svg-core": "^6.7.1",
		"@stencil/core": "4.29.0",
		"ag-grid-community": "^33.1.0",
		"ag-grid-react": "^33.1.0",
		"composed-offset-position": "^0.0.6",
		"dotenv": "^16.4.5",
		"init": "^0.1.2",
		"storybook": "^8.4.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.9.0",
		"@stencil-community/postcss": "^2.2.0",
		"@stencil/angular-output-target": "^0.10.2",
		"@stencil/eslint-plugin": "^1.1.0",
		"@stencil/react-output-target": "^1.0.2",
		"@stencil/sass": "^3.0.12",
		"@stencil/vue-output-target": "0.10.7",
		"@storybook/addon-a11y": "^8.4.5",
		"@storybook/addon-docs": "^8.4.5",
		"@storybook/addon-essentials": "^8.4.5",
		"@storybook/addon-links": "^8.4.5",
		"@storybook/addon-themes": "^8.6.14",
		"@storybook/addon-webpack5-compiler-swc": "^1.0.5",
		"@storybook/blocks": "^8.4.5",
		"@storybook/test": "^8.4.5",
		"@storybook/web-components": "^8.4.5",
		"@storybook/web-components-webpack5": "^8.4.5",
		"@tailwindcss/forms": "^0.5.9",
		"@testing-library/dom": "^8.11.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@types/jest": "^29.5.14",
		"@types/node": "^20.16.0",
		"@types/react": "18.2.33",
		"@types/react-dom": "18.2.14",
		"autoprefixer": "^10.4.20",
		"cpx": "^1.5.0",
		"cssnano": "^5.1.15",
		"eslint": "^9.33.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-jest": "^29.0.1",
		"eslint-plugin-jsx-a11y": "^6.10.2",
		"eslint-plugin-prettier": "^5.0.1",
		"eslint-plugin-storybook": "^0.10.0",
		"eslint-plugin-tailwindcss": "^3.18.2",
		"flag-icons": "^7.3.2",
		"globals": "^15.9.0",
		"jest": "^29.7.0",
		"jest-cli": "^29.7.0",
		"jiti": "^2.5.1",
		"lit": "^3.2.1",
		"puppeteer": "^21.11.0",
		"rollup": "^4.27.4",
		"rollup-plugin-sourcemaps": "^0.6.3",
		"stencil-tailwind-plugin": "^1.8.0",
		"style-dictionary": "^3.9.2",
		"tailwindcss": "^3.4.15",
		"ts-node": "^10.0.0",
		"tsx": "^4.15.4",
		"typescript": "~5.4.5",
		"typescript-eslint": "^8.0.1"
	},
	"license": "MIT",
	"publishConfig": {
		"registry": "https://oec-vsts.pkgs.visualstudio.com/UX/_packaging/oui/npm/registry/",
		"access": "restricted"
	}
}
